<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ReserveFacilities.Actions">
   <children>
      <BorderPane layoutX="-1.0" prefHeight="404.0" prefWidth="600.0">
         <left>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fitHeight="415.0" fitWidth="241.0" layoutY="-2.0" pickOnBounds="true">
                     <image>
                        <Image url="@images/green_background.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="4.0" layoutY="25.0" text="Facility Reservation" textFill="WHITE">
                     <font>
                        <Font name="Arial Black" size="21.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </left>
         <center>
            <AnchorPane prefHeight="426.0" prefWidth="366.0" BorderPane.alignment="CENTER">
               <children>
                  <Label layoutX="28.0" layoutY="41.0" text="Facility Name">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
                  <TextField fx:id="Facility_Name" layoutX="159.0" layoutY="35.0" promptText="Facility" style="-fx-border-color: #00573F; -fx-text-fill: #00573F; -fx-background-color: white; -fx-border-width: 0px 0px 2.5px 0px;" />
                  <Label layoutX="19.0" layoutY="76.0" text="Reservation Date">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
                  <DatePicker fx:id="Facility_Date" layoutX="159.0" layoutY="69.0" prefHeight="25.0" prefWidth="150.0" promptText="MM/DD/YY" style="-fx-border-color: #00573F; -fx-text-fill: #00573F; -fx-background-color: white; -fx-border-width: 0px 0px 2.5px 0px;" />
                  <Label layoutX="18.0" layoutY="115.0" text="Facility Location">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
                  <TextField fx:id="Facility_Location" layoutX="159.0" layoutY="109.0" prefHeight="25.0" prefWidth="148.0" promptText="Location" style="-fx-border-color: #00573F; -fx-text-fill: #00573F; -fx-background-color: white; -fx-border-width: 0px 0px 2.5px 0px;" />
                  <Label layoutX="30.0" layoutY="152.0" text="Time Start at">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
                  <TextField fx:id="Facility_TimeS" layoutX="159.0" layoutY="146.0" promptText="XX:XX (24 hours Format)" style="-fx-border-color: #00573F; -fx-text-fill: #00573F; -fx-background-color: white; -fx-border-width: 0px 0px 2.5px 0px;" />
                  <Button fx:id="Facility_Return" layoutX="34.0" layoutY="364.0" mnemonicParsing="false" style="-fx-background-color: #00573F;" text="Return" textFill="WHITE">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Button>
                  <Button fx:id="Facility_Apply" layoutX="281.0" layoutY="364.0" mnemonicParsing="false" style="-fx-background-color: #00573F;" text="Apply" textFill="WHITE">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Button>
                  <TextField fx:id="Facility_TimeE" layoutX="159.0" layoutY="183.0" promptText="XX:XX (24 hours Format)" style="-fx-border-color: #00573F; -fx-text-fill: #00573F; -fx-background-color: white; -fx-border-width: 0px 0px 2.5px 0px;" />
                  <Label layoutX="30.0" layoutY="189.0" text="Time Ends at">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
                  <TextArea fx:id="Details_M" layoutX="159.0" layoutY="219.0" prefHeight="137.0" prefWidth="167.0" promptText="Why you want to reserve this facility?">
                     <font>
                        <Font size="13.0" />
                     </font></TextArea>
                  <Label layoutX="7.0" layoutY="250.0" text="Reason of Reserving">
                     <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                     </font></Label>
               </children></AnchorPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
